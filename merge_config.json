    "just_file_prefixes": [],
    "just_file_contain": [],
    "any_file_prefixes": [],
    "any_file_contain": [],
    "search_keywords": [],
    "included_extensions": [
        ".rs",
        ".ts",
        ".tsx",
        ".css",
        ".scss",
        ".json",
